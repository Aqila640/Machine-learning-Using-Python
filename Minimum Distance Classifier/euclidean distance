import math
import numpy as np

# Example points in 3-dimensional space...
x = (0.1, 0.5, 0.1)
m1=(0,0,0)
m2=(0.5,0.5,0.5)
distance1 = math.sqrt(sum([(a - b) ** 2 for a, b in zip(m1, x)]))
distance2 = math.sqrt(sum([(a - b) ** 2 for a, b in zip(m2, x)]))
print("Euclidean distance from x to y: ",distance1+distance2)
